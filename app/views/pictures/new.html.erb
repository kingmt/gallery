<h1>New picture</h1>

<% form_for([@album, @picture], :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <p>
    <label for="picture">Upload A Picture:</label>
    <%= f.file_field :uploaded_data %>
  </p>

  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', album_pictures_path(@album) %>
